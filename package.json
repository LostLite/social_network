{
  "name": "social_network",
  "version": "1.0.0",
  "description": "Social network project with node and react js",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.19.3"
  },
  "scripts": {
    "db:migrate": "sequelize db:migrate --config database/config/config.json --migrations-path database/migrations",
    "db:migrate-undo": "sequelize db:migrate:undo:all --config database/config/config.json --migrations-path database/migrations",
    "heroku-build-script":"npm i --sava sequelize-cli && npm run db:migrate",
    "start:dev": "nodemon index.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
